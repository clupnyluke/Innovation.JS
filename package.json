{
	"name": "turbocad",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile build/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile build/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/theme": "^14.0.0",
		"@rollup/plugin-url": "^8.0.1",
		"@rollup/plugin-wasm": "^6.1.2",
		"@smui-extra/accordion": "7.0.0-beta.0",
		"@smui/drawer": "7.0.0-beta.0",
		"@smui/icon-button": "7.0.0-beta.0",
		"@smui/list": "7.0.0-beta.0",
		"@smui/menu": "7.0.0-beta.0",
		"@smui/ripple": "7.0.0-beta.0",
		"@smui/tab": "7.0.0-beta.0",
		"@smui/tab-bar": "7.0.0-beta.0",
		"@smui/top-app-bar": "7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@threlte/core": "^5.0.8",
		"@threlte/extras": "^4.9.2",
		"@threlte/preprocess": "^0.0.2",
		"@types/lodash": "^4.14.191",
		"@types/node": "^20.10.3",
		"@types/three": "^0.146.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lodash": "^4.17.21",
		"opencascade.js": "2.0.0-beta.b5ff984",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.56.2",
		"smui-theme": "7.0.0-beta.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-google-materialdesign-icons": "^0.7.1",
		"svelte-sequential-preprocessor": "^0.0.7",
		"three": "^0.147.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-wasm": "^3.1.1",
		"vitest": "^0.25.3",
		"ws": "^8.11.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.1.4",
		"@types/express": "^4.17.16",
		"chokidar": "^3.5.3",
		"esbuild": "^0.17.4",
		"express": "^4.18.2",
		"rollup-pluginutils": "^2.8.2",
		"socket.io": "^4.5.4",
		"socket.io-client": "^4.5.4",
		"vite-plugin-external": "^1.2.7"
	}
}
